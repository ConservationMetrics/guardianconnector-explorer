{
  "name": "guardianconnector-explorer",
  "version": "3.0.0",
  "type": "module",
  "description": "A Nuxt app to explore GuardianConnector data in different views",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "format": "prettier \"**/*.{js,ts,jsx,tsx,vue}\" --write",
    "generate": "nuxt generate",
    "link-module": "npm link ../gc-shared-components",
    "lint:prettier": "prettier ./ --check --ignore-path .prettierignore",
    "lint:eslint": "eslint . --cache",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest run",
    "prepare": "husky"
  },
  "dependencies": {
    "@turf/turf": "^7.1.0",
    "@vojtechlanka/vue-tags-input": "^3.1.1",
    "chart.js": "^4.4.4",
    "gc-shared-resources": "latest",
    "lodash": "^4.17.21",
    "mapbox-gl": "^3.4.0",
    "nuxt": "^3.13.0",
    "nuxt-auth-utils": "^0.3.6",
    "pg": "^8.12.0",
    "sqlite3": "^5.1.7",
    "tokml": "^0.4.0",
    "vue": "latest",
    "vue-3-slider-component": "^1.0.1",
    "vue-chartjs": "^5.3.1",
    "vue-datepicker-next": "^1.0.3",
    "vue-router": "latest",
    "vue3-select-component": "^0.6.1"
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx,json,css,vue}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@nuxt/test-utils": "^3.14.1",
    "@nuxtjs/i18n": "^8.5.5",
    "@types/mapbox-gl": "^3.1.0",
    "@types/node": "^22.5.4",
    "@types/pg": "^8.11.6",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.11.1",
    "eslint-config-flat-gitignore": "^0.3.0",
    "eslint-plugin-vue": "^9.28.0",
    "globals": "^15.9.0",
    "husky": "^9.0.11",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.7",
    "nuxt-windicss": "^2.6.1",
    "prettier": "^3.3.2",
    "typescript-eslint": "^8.7.0",
    "vitest": "^2.0.5"
  },
  "engines": {
    "node": "18.17.0"
  },
  "author": "Conservation Metrics",
  "license": "MIT"
}
