{
  "name": "guardianconnector-views",
  "version": "1.1.1",
  "type": "module",
  "description": "A Nuxt app to visualize GuardianConnector data in different views",
  "private": true,
  "scripts": {
    "dev": "./node_modules/.bin/nuxt",
    "build": "./node_modules/.bin/nuxt build",
    "start": "./node_modules/.bin/nuxt start",
    "generate": "./node_modules/.bin/nuxt generate",
    "lint": "prettier --write ./ --ignore-path .prettierignore",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nuxtjs/auth-next": "5.0.0-1667386184.dfbbb54",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/dotenv": "^1.4.2",
    "@nuxtjs/i18n": "^7.3.1",
    "@turf/turf": "^6.5.0",
    "chart.js": "^4.4.1",
    "core-js": "^3.35.0",
    "express": "^4.18.2",
    "hchs-vue-charts": "^1.2.31",
    "jsonwebtoken": "^9.0.2",
    "mapbox-gl": "^2.15.0",
    "nuxt": "^2.17.2",
    "pg": "^8.11.3",
    "sqlite3": "^5.1.6",
    "vue": "^2.7.10",
    "vue-chartjs": "^5.3.0",
    "vue-select": "^3.20.2",
    "vue-server-renderer": "^2.7.16",
    "vue-slider-component": "^3.2.24",
    "vue-template-compiler": "^2.7.16",
    "vue2-datepicker": "^3.11.1"
  },
  "devDependencies": {
    "@nuxt/types": "^2.17.2",
    "@nuxt/typescript-build": "^3.0.2",
    "@nuxt/typescript-runtime": "^3.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mapbox-gl": "^2.7.19",
    "@types/node-fetch": "^2.6.11",
    "@types/pg": "^8.10.9",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "nuxt-windicss": "^2",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,vue}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": "18.17.0"
  },
  "author": "Conservation Metrics",
  "license": "MIT"
}
